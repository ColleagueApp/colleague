{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": "error",
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "react/no-array-index-key": 1,
    "import/no-unresolved": 0
  }
}
